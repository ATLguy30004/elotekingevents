}
.food-card img {
  width: 100%;
  aspect-ratio: 1/1;
  object-fit: cover;
  display: block;
}
.food-card-name {
  font-size: 13px;
  font-weight: 700;
  color: var(--text-primary);
  padding: 8px 10px 6px;
  line-height: 1.2;
}
.food-card-tag {
  font-size: 11px;
  font-weight: 700;
  color: var(--green);
  background: var(--green-bg);
